local toolSpawnTimes = {}
local noclipConnection = nil

local function monitorTools()
    local cupsFolder = workspace:FindFirstChild("Cups")
    if cupsFolder then
        cupsFolder.ChildAdded:Connect(function(child)
            if child:IsA("Tool") then
                toolSpawnTimes[child] = tick()
            end
        end)
        
        cupsFolder.ChildRemoved:Connect(function(child)
            if toolSpawnTimes[child] then
                toolSpawnTimes[child] = nil
            end
        end)
        
        for _, tool in pairs(cupsFolder:GetChildren()) do
            if tool:IsA("Tool") then
                toolSpawnTimes[tool] = tick()
            end
        end
    end
    
    workspace.ChildAdded:Connect(function(child)
        if child:IsA("Tool") then
            toolSpawnTimes[child] = tick()
        end
    end)
    
    workspace.ChildRemoved:Connect(function(child)
        if toolSpawnTimes[child] then
            toolSpawnTimes[child] = nil
        end
    end)
    
    for _, child in pairs(workspace:GetChildren()) do
        if child:IsA("Tool") then
            toolSpawnTimes[child] = tick()
        end
    end
end

monitorTools()

local function getCollectableTools()
    local tools = {}
    local trollgeBoxes = {}
    local currentTime = tick()
    
    local cupsFolder = workspace:FindFirstChild("Cups")
    if cupsFolder then
        for _, tool in pairs(cupsFolder:GetChildren()) do
            if tool:IsA("Tool") then
                local spawnTime = toolSpawnTimes[tool] or 0
                if currentTime - spawnTime >= _G.WaitInterval then
                    local handle = tool:FindFirstChild("Handle")
                    if handle and handle:FindFirstChild("ProximityPrompt") then
                        if tool.Name == "TrollgeBox" then
                            table.insert(trollgeBoxes, tool)
                        else
                            table.insert(tools, tool)
                        end
                    end
                end
            end
        end
    end
    
    for _, child in pairs(workspace:GetChildren()) do
        if child:IsA("Tool") and child.Parent ~= cupsFolder then
            local spawnTime = toolSpawnTimes[child] or 0
            if currentTime - spawnTime >= _G.WaitInterval then
                local handle = child:FindFirstChild("Handle")
                if handle and handle:FindFirstChild("ProximityPrompt") then
                    if child.Name == "TrollgeBox" then
                        table.insert(trollgeBoxes, child)
                    else
                        table.insert(tools, child)
                    end
                end
            end
        end
    end
    
    for _, box in ipairs(trollgeBoxes) do
        table.insert(tools, 1, box)
    end
    
    return tools
end

local function enableFloat()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    
    if humanoidRootPart then
        if humanoidRootPart:FindFirstChild("BodyPosition") then
            humanoidRootPart:FindFirstChild("BodyPosition"):Destroy()
        end
        if humanoidRootPart:FindFirstChild("BodyGyro") then
            humanoidRootPart:FindFirstChild("BodyGyro"):Destroy()
        end
        
        local bodyPosition = Instance.new("BodyPosition")
        bodyPosition.MaxForce = Vector3.new(4000, 4000, 4000)
        bodyPosition.Position = humanoidRootPart.Position
        bodyPosition.Parent = humanoidRootPart
        
        local bodyGyro = Instance.new("BodyGyro")
        bodyGyro.MaxTorque = Vector3.new(4000, 4000, 4000)
        bodyGyro.CFrame = humanoidRootPart.CFrame
        bodyGyro.Parent = humanoidRootPart
    end
end

local function disableFloat()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    
    if humanoidRootPart then
        if humanoidRootPart:FindFirstChild("BodyPosition") then
            humanoidRootPart:FindFirstChild("BodyPosition"):Destroy()
        end
        
        if humanoidRootPart:FindFirstChild("BodyGyro") then
            humanoidRootPart:FindFirstChild("BodyGyro"):Destroy()
        end
    end
end

local function enableNoclip()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    noclipConnection = game:GetService("RunService").Stepped:Connect(function()
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") and part.CanCollide then
                    part.CanCollide = false
                end
            end
        end
    end)
end

local function disableNoclip()
    if noclipConnection then
        noclipConnection:Disconnect()
        noclipConnection = nil
    end
    
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    if character then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = true
            end
        end
    end
end

local function autoFarmLoop()
    while _G.AutoFarm do
        local player = game.Players.LocalPlayer
        local character = player.Character
        if not character then
            wait(1)
            continue
        end
        
        local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
        if not humanoidRootPart then
            wait(1)
            continue
        end
        
        if not humanoidRootPart:FindFirstChild("BodyPosition") or not humanoidRootPart:FindFirstChild("BodyGyro") then
            enableFloat()
        end
        
        local tools = getCollectableTools()
        
        for _, tool in pairs(tools) do
            if not _G.AutoFarm then break end
            
            local handle = tool:FindFirstChild("Handle")
            if handle and handle:FindFirstChild("ProximityPrompt") then
                local targetPosition = handle.Position - Vector3.new(0, _G.BelowDistance, 0)
                
                if humanoidRootPart:FindFirstChild("BodyPosition") then
                    humanoidRootPart.BodyPosition.Position = targetPosition
                end
                
                humanoidRootPart.CFrame = CFrame.new(targetPosition)
                
                wait(_G.TeleportInterval)
                
                local collected = false
                local startTime = tick()
                
                while not collected and _G.AutoFarm and (tick() - startTime) < 1.5 do
                    if not tool.Parent then
                        collected = true
                        break
                    end
                    
                    local currentTargetPosition = handle.Position - Vector3.new(0, _G.BelowDistance, 0)
                    
                    if humanoidRootPart:FindFirstChild("BodyPosition") then
                        humanoidRootPart.BodyPosition.Position = currentTargetPosition
                    end
                    
                    humanoidRootPart.CFrame = CFrame.new(currentTargetPosition)
                    
                    local prompt = handle:FindFirstChild("ProximityPrompt")
                    if prompt and (humanoidRootPart.Position - handle.Position).Magnitude < 10 then
                        fireproximityprompt(prompt)
                    end
                    
                    wait(0.1)
                end
                
                if collected or (tick() - startTime) >= 2.5 then
                    wait(_G.CollectInterval)
                end
            end
        end
        
        wait(0.5)
    end
end

local function killNPCsLoop()
    while _G.KillAllNPCs do
        for _, v in pairs(workspace:GetDescendants()) do
            if v:IsA("Model") and v:FindFirstChild("Humanoid") and not game.Players:GetPlayerFromCharacter(v) then
                pcall(function()
                    v.Humanoid.Health = 0
                end)
            end
        end
        wait(2)
    end
end

local function setCanTouch(enabled)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            pcall(function()
                part.CanTouch = enabled
            end)
        end
    end
end

local function removeCupTools()
    local function removeTool(tool)
        local toolName = tool.Name:lower()
        if toolName == "normalcup" or toolName == "bloodcup" then
            pcall(function()
                tool:Destroy()
            end)
        end
    end
    
    local cupsFolder = workspace:FindFirstChild("Cups")
    if cupsFolder then
        for _, child in pairs(cupsFolder:GetChildren()) do
            if child:IsA("Tool") then
                removeTool(child)
            end
        end
    end
    
    for _, child in pairs(workspace:GetChildren()) do
        if child:IsA("Tool") then
            removeTool(child)
        end
    end
end

CloseButton.MouseButton1Click:Connect(function()
    _G.AutoFarm = false
    _G.KillAllNPCs = false
    _G.CanTouchEnabled = true
    disableFloat()
    disableNoclip()
    WOTScriptGui:Destroy()
end)

AutoFarmButton.MouseButton1Click:Connect(function()
    _G.AutoFarm = not _G.AutoFarm
    if _G.AutoFarm then
        AutoFarmButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        AutoFarmButton.Text = "Auto Farm: ON"
        enableFloat()
        enableNoclip()
        spawn(autoFarmLoop)
    else
        AutoFarmButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        AutoFarmButton.Text = "Auto Farm: OFF"
        disableFloat()
        disableNoclip()
    end
end)

KillNPCsButton.MouseButton1Click:Connect(function()
    _G.KillAllNPCs = not _G.KillAllNPCs
    if _G.KillAllNPCs then
        KillNPCsButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        KillNPCsButton.Text = "Kill NPCs: ON"
        spawn(killNPCsLoop)
    else
        KillNPCsButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        KillNPCsButton.Text = "Kill NPCs: OFF"
    end
end)

CanTouchButton.MouseButton1Click:Connect(function()
    _G.CanTouchEnabled = not _G.CanTouchEnabled
    if _G.CanTouchEnabled then
        CanTouchButton.BackgroundColor3 = Color3.fromRGB(50, 200, 50)
        CanTouchButton.Text = "CanTouch: ON"
    else
        CanTouchButton.BackgroundColor3 = Color3.fromRGB(220, 50, 50)
        CanTouchButton.Text = "CanTouch: OFF"
    end
    setCanTouch(_G.CanTouchEnabled)
end)

TpToHell5Button.MouseButton1Click:Connect(function()
    game:GetService("TeleportService"):Teleport(75661637882183)
end)

GetFallenButton.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
    
    local referenceParts = workspace:FindFirstChild("ReferenceParts")
    if referenceParts then
        local fallenFolder = referenceParts:FindFirstChild("Fallen statue")
        if fallenFolder then
            local fallenModel = fallenFolder:FindFirstChild("Fallen statue")
            if fallenModel then
                local torso = fallenModel:FindFirstChild("Torso")
                if torso then
                    humanoidRootPart.CFrame = torso.CFrame + Vector3.new(0, 3, 0)
                    wait(0.5)
                    local prompt = torso:FindFirstChildOfClass("ProximityPrompt")
                    if prompt then
                        fireproximityprompt(prompt)
                    end
                end
            end
        end
    end
end)

CollectInput:GetPropertyChangedSignal("Text"):Connect(function()
    local value = tonumber(CollectInput.Text)
    if value then
        _G.CollectInterval = value
    else
        CollectInput.Text = tostring(_G.CollectInterval)
    end
end)

