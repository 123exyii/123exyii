game.Players.LocalPlayer.CharacterAdded:Connect(function(character)
    wait(1)
    if _G.AutoFarm then
        enableFloat()
        enableNoclip()
    end
    setCanTouch(_G.CanTouchEnabled)
end)

removeCupTools()
